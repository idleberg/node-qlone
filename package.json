{
  "name": "qlone",
  "version": "0.4.0",
  "description": "CLI tool to clone repositories, install dependencies and run scripts. All in one command.",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "qlone": "./index.js"
  },
  "scripts": {
    "build": "tsc --pretty --project ./",
    "dev": "tsc --watch --pretty --project ./",
    "lint": "tslint --project ./",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "author": "Jan T. Sott",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/node-qlone.git"
  },
  "keywords": [
    "git clone",
    "install dependencies"
  ],
  "types": "./types",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@samverschueren/stream-to-observable": "^0.3.0",
    "commander": "^2.17.0",
    "execa": "^0.10.0",
    "listr": "^0.14.1",
    "log-symbols": "^2.2.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.0",
    "split": "^1.0.1",
    "update-notifier": "^2.5.0",
    "util": "^0.11.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.5",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
